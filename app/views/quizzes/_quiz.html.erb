<div id="<%= dom_id quiz %>">
  <p>
    <strong>Title:</strong>
    <%= quiz.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= quiz.content %>
  </p>

  <p>
    <strong>Slug:</strong>
    <%= quiz.slug %>
  </p>

  <p>
    <strong>Course:</strong>
    <%= quiz.course_id %>
  </p>

  <p>
    <strong>User:</strong>
    <%= quiz.user_id %>
  </p>

</div>
